#
# tenure based, trivial memory load is ignored
#
MaxT ;    ; tenure; safemax
SumT ;    ; tenure; sum
AvgT ;    ; tenure; avg

# tenure based, trivial memory load is not ignored
#
MaxT' ;   ; tenure; safemax; trivial
SumT' ;   ; tenure; sum    ; trivial
AvgT' ;    ; tenure; avg ; trivial
